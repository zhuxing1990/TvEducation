<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipChildren="false"
    android:clipToPadding="false"
    android:orientation="vertical">

        <com.vunke.education.view.KuangRelativeLayout
            android:id="@+id/main_relative_watch_history"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="8dp"
            android:paddingBottom="8dp"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true">

            <RelativeLayout
                android:layout_width="202dp"
                android:layout_height="120dp"
                android:clipChildren="false"
                android:clipToPadding="false"
                android:background="@color/color_brown">

                <com.vunke.education.view.RoundAngleImageView
                    android:layout_width="66dp"
                    android:layout_height="59dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="14dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/watch_history" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_alignParentBottom="true"
                    android:paddingBottom="12dp"
                    android:text="@string/watch_history"
                    android:textColor="@color/color_text_white2"
                    android:textSize="22sp" />
            </RelativeLayout>
        </com.vunke.education.view.KuangRelativeLayout>

        <com.vunke.education.view.KuangRelativeLayout
            android:layout_marginLeft="5dp"
            android:id="@+id/main_relative_ranking_list"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="8dp"
            android:paddingBottom="8dp"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:layout_toRightOf="@+id/main_relative_watch_history">

            <RelativeLayout
                android:layout_width="202dp"
                android:layout_height="120dp"
                android:clipChildren="false"
                android:clipToPadding="false"
                android:background="@color/color_turquoise">

                <com.vunke.education.view.RoundAngleImageView
                    android:layout_width="66dp"
                    android:layout_height="59dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="14dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/ranking_list" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_alignParentBottom="true"
                    android:paddingBottom="12dp"
                    android:text="@string/ranking_list"
                    android:textColor="@color/color_text_white2"
                    android:textSize="22sp" />
            </RelativeLayout>
        </com.vunke.education.view.KuangRelativeLayout>

        <RelativeLayout
            android:layout_width="234dp"
            android:layout_height="144dp"
            android:id="@+id/main_small_relative1"
            android:focusable="false"
            android:layout_marginLeft="10dp"
            android:gravity="center_vertical"
            android:layout_toRightOf="@+id/main_relative_ranking_list">
            <com.vunke.education.view.KuangRelativeLayout
                android:id="@+id/main_ralative_small_view1"
                android:layout_width="105dp"
                android:layout_height="144dp"
                android:padding="10dp"
                android:layout_centerVertical="true">
                <com.vunke.education.view.RoundAngleImageView
                    android:id="@+id/main_small_view1"
                    android:layout_width="74dp"
                    android:layout_height="74dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="10dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_marginBottom="5dp"
                    android:text="@string/children_education_graphics"
                    android:textColor="@color/color_text_white2"
                    android:singleLine="true"
                    android:textSize="18sp"
                    />
            </com.vunke.education.view.KuangRelativeLayout>



            <com.vunke.education.view.KuangRelativeLayout
                android:id="@+id/main_ralative_small_view2"
                android:layout_width="105dp"
                android:layout_height="144dp"
                android:padding="10dp"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/main_ralative_small_view1"
                android:layout_centerVertical="true">
                <com.vunke.education.view.RoundAngleImageView
                    android:id="@+id/main_small_view2"
                    android:layout_width="74dp"
                    android:layout_height="74dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="10dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/children_education_games"
                    android:textColor="@color/color_text_white2"
                    android:layout_alignParentBottom="true"
                    android:singleLine="true"
                    android:layout_marginBottom="5dp"
                    android:textSize="18sp"/>
            </com.vunke.education.view.KuangRelativeLayout>
        </RelativeLayout>


        <RelativeLayout
            android:layout_width="234dp"
            android:layout_height="144dp"
            android:id="@+id/main_small_relative2"
            android:focusable="false"
            android:gravity="center_vertical"
            android:layout_toRightOf="@+id/main_small_relative1">
            <com.vunke.education.view.KuangRelativeLayout
                android:id="@+id/main_ralative_small_view3"
                android:layout_width="105dp"
                android:layout_height="144dp"
                android:padding="10dp"
                android:layout_centerVertical="true">
                <com.vunke.education.view.RoundAngleImageView
                    android:id="@+id/main_small_view3"
                    android:layout_width="74dp"
                    android:layout_height="74dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="10dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:text="@string/children_education_graphics"
                    android:textColor="@color/color_text_white2"
                    android:singleLine="true"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp"
                    />
            </com.vunke.education.view.KuangRelativeLayout>

            <com.vunke.education.view.KuangRelativeLayout
                android:id="@+id/main_ralative_small_view4"
                android:layout_width="105dp"
                android:layout_height="144dp"
                android:padding="10dp"
                android:layout_toRightOf="@+id/main_ralative_small_view3"
                android:layout_centerVertical="true">
                <com.vunke.education.view.RoundAngleImageView
                    android:id="@+id/main_small_view4"
                    android:layout_width="74dp"
                    android:layout_height="74dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="10dp"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/children_education_games"
                    android:textColor="@color/color_text_white2"
                    android:layout_alignParentBottom="true"
                    android:singleLine="true"
                    android:layout_marginBottom="5dp"
                    android:textSize="18sp"/>
            </com.vunke.education.view.KuangRelativeLayout>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="234dp"
            android:layout_height="150dp"
            android:id="@+id/main_small_relative3"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:layout_toRightOf="@id/main_small_relative2">
            <com.vunke.education.view.KuangRelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingRight="12dp"
                android:paddingLeft="12dp"
                android:paddingTop="4dp"
                android:paddingBottom="4dp"
                android:layout_centerInParent="true"
                android:id="@+id/main_ordering">
                <com.vunke.education.view.RoundAngleImageView
                    android:layout_width="160dp"
                    android:layout_height="60dp"
                    android:layout_centerInParent="true"
                    android:src="@drawable/ordering"/>
            </com.vunke.education.view.KuangRelativeLayout>
        </RelativeLayout>
</RelativeLayout>