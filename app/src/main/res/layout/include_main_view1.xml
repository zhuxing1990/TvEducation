<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipChildren="false"
    android:clipToPadding="false">

        <RelativeLayout
            android:layout_width="434dp"
            android:layout_height="282dp"
            android:layout_marginTop="5dp"
            android:id="@+id/main_surfaceview">
            <SurfaceView
                android:id="@+id/main_surfaceView"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
        </RelativeLayout>

        <com.vunke.education.view.KuangRelativeLayout
            android:id="@+id/main_relative_view1"
            android:layout_width="234dp"
            android:layout_height="145dp"
            android:paddingTop="8dp"
            android:paddingBottom="8dp"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:layout_marginLeft="10dp"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:layout_toRightOf="@id/main_surfaceview">
            <com.vunke.education.view.RoundAngleImageView
                android:id="@+id/main_big_view1"
                android:layout_width="220dp"
                android:layout_height="137dp"
                android:scaleType="fitXY"
                android:layout_centerInParent="true"
                />
        </com.vunke.education.view.KuangRelativeLayout>
        <com.vunke.education.view.KuangRelativeLayout
            android:id="@+id/main_relative_view2"
            android:layout_width="234dp"
            android:layout_height="145dp"
            android:paddingTop="8dp"
            android:paddingBottom="8dp"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:layout_toRightOf="@id/main_relative_view1">
            <com.vunke.education.view.RoundAngleImageView
                android:id="@+id/main_big_view2"
                android:layout_width="220dp"
                android:layout_height="137dp"
                android:scaleType="fitXY"
                />
        </com.vunke.education.view.KuangRelativeLayout>
        <com.vunke.education.view.KuangRelativeLayout
            android:id="@+id/main_relative_view3"
            android:layout_width="234dp"
            android:layout_height="145dp"
            android:paddingTop="8dp"
            android:paddingBottom="8dp"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:layout_toRightOf="@id/main_relative_view2">
            <com.vunke.education.view.RoundAngleImageView
                android:id="@+id/main_big_view3"
                android:layout_width="220dp"
                android:layout_height="137dp"
                android:scaleType="fitXY"
                />
        </com.vunke.education.view.KuangRelativeLayout>

        <com.vunke.education.view.KuangRelativeLayout
            android:id="@+id/main_relative_view4"
            android:layout_width="234dp"
            android:layout_height="145dp"
            android:paddingTop="8dp"
            android:paddingBottom="8dp"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="10dp"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:layout_below="@+id/main_relative_view1"
            android:layout_toRightOf="@id/main_surfaceview">
            <com.vunke.education.view.RoundAngleImageView
                android:id="@+id/main_big_view4"
                android:layout_width="220dp"
                android:layout_height="137dp"
                android:scaleType="fitXY"
                />
        </com.vunke.education.view.KuangRelativeLayout>
        <com.vunke.education.view.KuangRelativeLayout
            android:id="@+id/main_relative_view5"
            android:layout_width="234dp"
            android:layout_height="145dp"
            android:paddingTop="8dp"
            android:paddingBottom="8dp"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:layout_marginTop="5dp"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:layout_below="@+id/main_relative_view2"
            android:layout_toRightOf="@id/main_relative_view4">
            <com.vunke.education.view.RoundAngleImageView
                android:id="@+id/main_big_view5"
                android:layout_width="220dp"
                android:layout_height="137dp"
                android:scaleType="fitXY"
                />
        </com.vunke.education.view.KuangRelativeLayout>
        <com.vunke.education.view.KuangRelativeLayout
            android:id="@+id/main_relative_view6"
            android:layout_width="234dp"
            android:layout_height="145dp"
            android:paddingTop="8dp"
            android:paddingBottom="8dp"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:layout_marginTop="5dp"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:layout_below="@+id/main_relative_view3"
            android:layout_toRightOf="@id/main_relative_view5">
            <com.vunke.education.view.RoundAngleImageView
                android:id="@+id/main_big_view6"
                android:layout_width="220dp"
                android:layout_height="137dp"
                android:scaleType="fitXY"
                />
        </com.vunke.education.view.KuangRelativeLayout>

</RelativeLayout>