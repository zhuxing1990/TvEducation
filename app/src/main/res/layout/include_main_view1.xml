<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <SurfaceView
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:id="@+id/main_surfaceview"
        android:layout_width="434dp"
        android:layout_height="267dp" />
    <com.vunke.education.view.KuangRelativeLayout
        android:id="@+id/main_relative_view1"
        android:layout_width="234dp"
        android:layout_height="127dp"
        android:layout_marginLeft="15dp"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:clickable="true"
        android:layout_toRightOf="@id/main_surfaceview">
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:duplicateParentState="true"
            android:scaleType="fitXY"
            android:src="@drawable/main_view1"/>
    </com.vunke.education.view.KuangRelativeLayout>
    <com.vunke.education.view.KuangRelativeLayout
        android:id="@+id/main_relative_view2"
        android:layout_width="234dp"
        android:layout_height="127dp"
        android:layout_marginLeft="15dp"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:clickable="true"
        android:layout_toRightOf="@id/main_relative_view1">
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:duplicateParentState="true"
            android:scaleType="fitXY"
            android:src="@drawable/main_view2"/>
    </com.vunke.education.view.KuangRelativeLayout>
    <com.vunke.education.view.KuangRelativeLayout
        android:id="@+id/main_relative_view3"
        android:layout_width="234dp"
        android:layout_height="127dp"
        android:layout_marginLeft="15dp"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:clickable="true"
        android:layout_toRightOf="@id/main_relative_view2">
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:duplicateParentState="true"
            android:scaleType="fitXY"
            android:src="@drawable/main_view3"/>
    </com.vunke.education.view.KuangRelativeLayout>

    <com.vunke.education.view.KuangRelativeLayout
        android:id="@+id/main_relative_view4"
        android:layout_width="234dp"
        android:layout_height="127dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="15dp"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:clickable="true"
        android:layout_below="@+id/main_relative_view1"
        android:layout_toRightOf="@id/main_surfaceview">
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:duplicateParentState="true"
            android:scaleType="fitXY"
            android:src="@drawable/main_view4"/>
    </com.vunke.education.view.KuangRelativeLayout>
    <com.vunke.education.view.KuangRelativeLayout
        android:id="@+id/main_relative_view5"
        android:layout_width="234dp"
        android:layout_height="127dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="15dp"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:clickable="true"
        android:layout_below="@+id/main_relative_view2"
        android:layout_toRightOf="@id/main_relative_view4">
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:duplicateParentState="true"
            android:scaleType="fitXY"
            android:src="@drawable/main_view5"/>
    </com.vunke.education.view.KuangRelativeLayout>
    <com.vunke.education.view.KuangRelativeLayout
        android:id="@+id/main_relative_view6"
        android:layout_width="234dp"
        android:layout_height="127dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="15dp"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:clickable="true"
        android:layout_below="@+id/main_relative_view3"
        android:layout_toRightOf="@id/main_relative_view5">
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:duplicateParentState="true"
            android:scaleType="fitXY"
            android:src="@drawable/main_view6"/>
    </com.vunke.education.view.KuangRelativeLayout>
</RelativeLayout>